    <!-- http://www.semanticweb.org/herus/Focus/interview544 -->

    <owl:NamedIndividual rdf:about="&Focus;interview544">
        <rdf:type rdf:resource="&Focus;interview"/>
        <Focus:code>Motivation for engagement</Focus:code>
        <Focus:content xml:lang="en">I was intrigued by the, this possibility to work together with architects and engineers to see � or civil engineers � to see how they work.</Focus:content>
