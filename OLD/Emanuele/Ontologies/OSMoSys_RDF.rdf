        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasInterval"/>
                <owl:someValuesFrom rdf:resource="&time;DateTimeInterval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasOutput"/>
                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#SensorOutput"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasPlatform"/>
