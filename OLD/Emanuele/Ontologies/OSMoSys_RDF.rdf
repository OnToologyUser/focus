    <!-- http://xmlns.com/foaf/0.1/Group -->

    <owl:Class rdf:about="&foaf;/Group">
        <rdfs:subClassOf rdf:resource="&DUL;CollectiveAgent"/>
        <dct:description xml:lang="en">A group of Persons and/or Organizations that share common characteristics such as Place, circumstance, etc.</dct:description>
        <rdfs:comment xml:lang="en">The Group class represents a collection of individual agents and may itself play the role of an Agent, i.e. something that can perform actions. While a Group has the characteristics of an Agent, it is also associated with a number of other Agents (typically people) who constitute the Group. </rdfs:comment>
    </owl:Class>
    


