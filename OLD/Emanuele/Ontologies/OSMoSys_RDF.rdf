            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#produces"/>
                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#SensorOutput"/>
            </owl:Restriction>
