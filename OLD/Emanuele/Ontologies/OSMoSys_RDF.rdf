            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ontopolis.org/osmosys#WebEntity"/>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#includes"/>
                                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#SensorNetwork"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#includes"/>
                                <owl:onClass rdf:resource="http://www.ontopolis.org/osmosys#SensorResource"/>
                                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
