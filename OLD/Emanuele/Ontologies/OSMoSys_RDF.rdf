    <!-- http://dbpedia.org/ontology/City -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/City">
        <rdfs:subClassOf rdf:resource="http://schema.org/AdministrativeArea"/>
        <rdfs:comment xml:lang="en">A relatively large and permanent settlement, particularly a large urban settlement</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest -->

    <owl:Class rdf:about="&ssn;FeatureOfInterest">
        <rdfs:subClassOf rdf:resource="&dct;Event"/>
        <rdfs:comment xml:lang="en">A FeatureOfInterest is an abstraction of real world phenomena (thing, person, event, etc).</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#Observation -->

    <owl:Class rdf:about="&ssn;Observation">
        <rdfs:subClassOf rdf:resource="&DUL;Situation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasTime"/>
                <owl:allValuesFrom rdf:resource="&time;TemporalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasFeatureOfInterest"/>
                <owl:onClass rdf:resource="&ssn;FeatureOfInterest"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DUL;includesEvent"/>
                <owl:someValuesFrom rdf:resource="&ssn;Stimulus"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasFeatureOfInterest"/>
                <owl:allValuesFrom rdf:resource="&ssn;FeatureOfInterest"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#observedBy"/>
                <owl:allValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#SensorResource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An Observation is a Situation in which a Sensing method has been used to estimate or calculate a value of a FeatureOfInterest. Links to Sensing and SensorResources describe what made the Observation and how; links to FeatureOfInterest detail what was sensed; the result is the output of a SensorResource; other metadata details times etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#Platform -->

    <owl:Class rdf:about="&ssn;Platform">
        <rdfs:subClassOf rdf:resource="&dct;PhysicalObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasLocation"/>
                <owl:someValuesFrom rdf:resource="http://schema.org/Place"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#attachesTo"/>
                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#SensorResource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ontopolis.org/osmosys#SocialMediaPlatform"/>
        <rdfs:comment xml:lang="en">A PhysicalObject to which other PhysicalObjects can be attached - particuarly Sensors and other Platforms. For example, a post might act as the Platform, a bouy might act as a Platform, or a fish might act as a Platform for an attached sensor.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#Process -->

    <owl:Class rdf:about="&ssn;Process">
        <rdfs:subClassOf rdf:resource="&DUL;Method"/>
        <dct:source>http://www.w3.org/2005/Incubator/ssn/</dct:source>
        <rdfs:seeAlso>http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Model#Process</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">A process has an output and possibly inputs and, for a composite process, describes the temporal and dataflow dependencies and relationships amongst its parts.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#Sensing -->

    <owl:Class rdf:about="&ssn;Sensing">
        <rdfs:subClassOf rdf:resource="&ssn;Process"/>
        <dct:source>http://www.w3.org/2005/Incubator/ssn/</dct:source>
        <rdfs:comment xml:lang="en">Sensing is a process that results in the estimation, or calculation, of the value of a phenomenon.</rdfs:comment>
        <rdfs:seeAlso>http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton</rdfs:seeAlso>
        <rdfs:isDefinedBy>http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#SensingDevice -->

    <owl:Class rdf:about="&ssn;SensingDevice">
        <rdfs:subClassOf rdf:resource="&ssn;Sensor"/>
        <rdfs:seeAlso>http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#Measuring</rdfs:seeAlso>
        <dct:source>http://www.w3.org/2005/Incubator/ssn/</dct:source>
        <rdfs:isDefinedBy>http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">A sensing device is a device that implements sensing.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#Sensor -->

    <owl:Class rdf:about="&ssn;Sensor">
        <rdfs:subClassOf rdf:resource="http://www.ontopolis.org/osmosys#SensorResource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#implements"/>
                <owl:someValuesFrom rdf:resource="&ssn;Sensing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ssn;detects"/>
                <owl:allValuesFrom rdf:resource="&ssn;Stimulus"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A sensor can do (implements) sensing: that is, a sensor is any entity that can follow a sensing method and thus observe some Property of a FeatureOfInterest.  Sensors may be physical devices, computational methods, a laboratory setup with a person following a method, a person or any other thing that can follow a Sensing Method to observe a Property.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
        <rdfs:seeAlso>http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton</rdfs:seeAlso>
        <dct:source>skos:exactMatch &apos;sensor&apos; [SensorML OGC-0700]
http://www.opengeospatial.org/standards/sensorml 

skos:closeMatch &apos;observation procedure&apos; [O&amp;M] 
http://www.opengeospatial.org/standards/om 

O&amp;M allows sensors, methods, instruments, systems, algorithms and process chains as the processUsed of an observation; this ontology allows a similar range of things (any thing that can do sensing), just they are all grouped under the term sensor (which is thus wider than the O&amp;M concept).</dct:source>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#SensorInput -->

    <owl:Class rdf:about="&ssn;SensorInput">
        <owl:equivalentClass rdf:resource="&ssn;Stimulus"/>
        <rdfs:subClassOf rdf:resource="&dct;Event"/>
        <rdfs:subClassOf rdf:resource="&DUL;InformationObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasTime"/>
                <owl:allValuesFrom rdf:resource="&time;TemporalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasSampling"/>
                <owl:allValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#DynamicSampling"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#refersTo"/>
                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#UrbanSystems"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ontopolis.org/osmosys#SensorOutput"/>
        <dct:source>http://www.w3.org/2005/Incubator/ssn/</dct:source>
        <rdfs:seeAlso>http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">An Event in the real world that &apos;triggers&apos; a Sensor. The properties associated to the stimulus may be different to eventual observed property. It is the Event that triggers the sensor.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#Stimulus -->

    <owl:Class rdf:about="&ssn;Stimulus">
        <rdfs:subClassOf rdf:resource="&dct;Event"/>
        <dct:source>http://www.w3.org/2005/Incubator/ssn/</dct:source>
        <rdfs:seeAlso>http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">An Event in the real world that &apos;triggers&apos; a Sensor. The properties associated to the stimulus may be different to eventual observed property. It is the Event that triggers the sensor.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://purl.org/dc/terms/#Event -->

    <owl:Class rdf:about="&dct;Event">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DUL;hasParticipant"/>
                <owl:someValuesFrom rdf:resource="&foaf;/Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#atPlace"/>
                <owl:someValuesFrom rdf:resource="http://schema.org/Place"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DUL;hasEventDate"/>
                <owl:someValuesFrom rdf:resource="&xsd;dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DUL;hasConstituent"/>
                <owl:allValuesFrom rdf:resource="&dct;Event"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasOutcome"/>
                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#Decision"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:seeAlso>http://dbpedia.org/ontology/Event</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">A non-persistent, time-based occurrence.</rdfs:comment>
        <dct:description xml:lang="en">An Event can refer to a number of Items that take roles in the Event, for example, a Person may take the role of applicant, in one Event, and the same person may take the role of patient, in another Event.</dct:description>
        <dct:source>British Standards Institution. (2014). PAS 182: Smart City Concept Model - Guide to Establishing a Model for Data Interoperability. London: BSI Standards Limited.</dct:source>
    </owl:Class>
    


    <!-- http://purl.org/dc/terms/#PhysicalObject -->

    <owl:Class rdf:about="&dct;PhysicalObject">
        <rdfs:subClassOf rdf:resource="http://www.ontopolis.org/osmosys#Item"/>
        <owl:disjointWith rdf:resource="http://www.ontopolis.org/osmosys#WebEntity"/>
    </owl:Class>
    


    <!-- http://schema.org/AdministrativeArea -->

    <owl:Class rdf:about="http://schema.org/AdministrativeArea">
        <rdfs:subClassOf rdf:resource="http://schema.org/Place"/>
        <rdfs:comment xml:lang="en">A geographical region under the jurisdiction of a particular government.</rdfs:comment>
    </owl:Class>
    


    <!-- http://schema.org/Place -->

    <owl:Class rdf:about="http://schema.org/Place">
        <dct:description xml:lang="en">A Place might have definite or indefinite boundaries. Geographic spaces can be a position, line, area, or volume.</dct:description>
        <rdfs:comment xml:lang="en">A geographic or virtual part of space.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#Action -->

    <owl:Class rdf:about="&DUL;Action">
        <rdfs:subClassOf rdf:resource="&dct;Event"/>
        <rdfs:comment xml:lang="en">An Event with at least one Agent that isParticipantIn it, and that executes a Task that typically isDefinedIn a Plan, Workflow, Project, etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#CollectiveAgent -->

    <owl:Class rdf:about="&DUL;CollectiveAgent">
        <rdfs:subClassOf rdf:resource="&foaf;/Agent"/>
        <rdfs:comment xml:lang="en">A SocialAgent that is actedBy Agents that are (and act as) members of a Collective. A collective agent can have roles that are also roles of those agents.
For example, in sociology, a &apos;group action&apos; is the situation in which a number of people (that result to be members of a collective) in a given area behave in a coordinated way in order to achieve a (often common) goal. The Agent in such a Situation is not single, but a CollectiveAgent (a Group). This can be generalized to the notion of social movement, which assumes a large Community or even the entire Society as agents.
The difference between a CollectiveAgent and an Organization is that a Description that introduces a CollectiveAgent is also one that unifies the corresponding Collective. In practice, this difference makes collective agents &apos;less stable&apos; than organizations, because they have a dedicated, publicly recognizable Description that is conceived to introduce them.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#Community -->

    <owl:Class rdf:about="&DUL;Community">
        <rdfs:subClassOf rdf:resource="&DUL;CollectiveAgent"/>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#InformationObject -->

    <owl:Class rdf:about="&DUL;InformationObject">
        <rdfs:subClassOf rdf:resource="http://www.ontopolis.org/osmosys#Item"/>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#Method -->

    <owl:Class rdf:about="&DUL;Method">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#implementedIn"/>
                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#Service"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A method is a Description that defines or uses concepts in order to guide carrying out actions aimed at a solution with respect to a problem. 
It is different from a Plan, because plans could be carried out in order to follow a method, but a method can be followed by executing alternative plans.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#Situation -->

    <owl:Class rdf:about="&DUL;Situation"/>
    


    <!-- http://www.ontopolis.org/osmosys#AmbientSensing -->

    <owl:Class rdf:about="http://www.ontopolis.org/osmosys#AmbientSensing">
        <rdfs:label xml:lang="en">Ambient Sensing</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#includes"/>
                        <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#SensorResource"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#isEmbeddedAt"/>
                        <owl:someValuesFrom rdf:resource="http://schema.org/Place"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ontopolis.org/osmosys#UrbanSensing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#isAbout"/>
                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#UrbanSystems"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Existing sensor networks distributed throughout an urban environment.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontopolis.org/osmosys#BuiltEnvironment -->

    <owl:Class rdf:about="http://www.ontopolis.org/osmosys#BuiltEnvironment">
        <rdfs:subClassOf rdf:resource="http://www.ontopolis.org/osmosys#UrbanSystems"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ontopolis.org/osmosys#Healthcare"/>
                    <rdf:Description rdf:about="http://www.ontopolis.org/osmosys#PolicingAndEmergency"/>
                    <rdf:Description rdf:about="http://www.ontopolis.org/osmosys#PublicSafety"/>
                    <rdf:Description rdf:about="http://www.ontopolis.org/osmosys#SocialServices"/>
                    <rdf:Description rdf:about="http://www.ontopolis.org/osmosys#Telecommunications"/>
                    <rdf:Description rdf:about="http://www.ontopolis.org/osmosys#Transport"/>
                    <rdf:Description rdf:about="http://www.ontopolis.org/osmosys#Waste"/>
                    <rdf:Description rdf:about="http://www.ontopolis.org/osmosys#Water"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasService"/>
                        <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#EnvironmentalServices"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ontopolis.org/osmosys#Connectivity"/>
                    <rdf:Description rdf:about="http://www.ontopolis.org/osmosys#DataAnalytics"/>
                    <rdf:Description rdf:about="http://www.ontopolis.org/osmosys#DataManagement"/>
                    <rdf:Description rdf:about="http://www.ontopolis.org/osmosys#InstrumentationAndMonitoring"/>
                    <rdf:Description rdf:about="http://www.ontopolis.org/osmosys#Interoperability"/>
                    <rdf:Description rdf:about="http://www.ontopolis.org/osmosys#SecurityAndPrivacy"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasTechnologyEnabler"/>
                        <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#ComputingResources"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#isSubSystemOf"/>
                <owl:someValuesFrom rdf:resource="http://dbpedia.org/ontology/City"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasTechnologyEnabler"/>
                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#SmartCityTechnologyEnablers"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasProcess"/>
                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#KeyCrossCityProcesses"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#measuredBySensor"/>
                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#SensorResource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasData"/>
                <owl:allValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#BuiltEnvironmentData"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dct:source>Berst, Jesse. (2014). Smart Cities Readiness Guide. Smart Cities Council</dct:source>
        <rdfs:comment xml:lang="en">BuiltEnvironment refers to all of a city’s buildings, parks and public spaces. Certain components of the BuiltEnvironment – including streets and utility infrastructure – are not emphasized here because they are treated in other responsibilities (Transport and Energy).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontopolis.org/osmosys#BuiltEnvironmentData -->

    <owl:Class rdf:about="http://www.ontopolis.org/osmosys#BuiltEnvironmentData">
        <rdfs:subClassOf rdf:resource="http://www.ontopolis.org/osmosys#Semi-StaticSampling"/>
        <rdfs:subClassOf rdf:resource="http://www.ontopolis.org/osmosys#UrbanSystemsData"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasOwner"/>
                <owl:allValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#Municipality"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ontopolis.org/osmosys#BusinessManagement -->

    <owl:Class rdf:about="http://www.ontopolis.org/osmosys#BusinessManagement">
        <rdfs:label xml:lang="en">Business Management</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.ontopolis.org/osmosys#GovernanceAndDeliveryProcesses"/>
    </owl:Class>
    


    <!-- http://www.ontopolis.org/osmosys#Case -->

    <owl:Class rdf:about="http://www.ontopolis.org/osmosys#Case">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#atPlace"/>
                <owl:someValuesFrom rdf:resource="http://schema.org/Place"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#raisedFrom"/>
                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#Service"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#contains"/>
                <owl:someValuesFrom rdf:resource="&dct;Event"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A container for information recording the history of Events initiated by a Service demand.</rdfs:comment>
        <dct:source>British Standards Institution. (2014). PAS 182: Smart City Concept Model - Guide to Establishing a Model for Data Interoperability. London: BSI Standards Limited.</dct:source>
    </owl:Class>
    


    <!-- http://www.ontopolis.org/osmosys#CityEvents -->

    <owl:Class rdf:about="http://www.ontopolis.org/osmosys#CityEvents">
        <rdfs:subClassOf rdf:resource="http://www.ontopolis.org/osmosys#Leisure"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#hasFeatureOfInterest"/>
                <owl:allValuesFrom rdf:resource="&ssn;FeatureOfInterest"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ontopolis.org/osmosys#ComputingResources -->

    <owl:Class rdf:about="http://www.ontopolis.org/osmosys#ComputingResources">
        <rdfs:subClassOf rdf:resource="http://www.ontopolis.org/osmosys#SmartCityTechnologyEnablers"/>
        <dct:source>Berst, Jesse. (2014). Smart Cities Readiness Guide. Smart Cities Council</dct:source>
        <rdfs:comment xml:lang="en">Computing resources include 1) the computer “brains” themselves, 2) storage of data and 3) special capabilities needed for smart cities.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontopolis.org/osmosys#Connectivity -->

    <owl:Class rdf:about="http://www.ontopolis.org/osmosys#Connectivity">
        <rdfs:subClassOf rdf:resource="http://www.ontopolis.org/osmosys#SmartCityTechnologyEnablers"/>
        <dct:source>Berst, Jesse. (2014). Smart Cities Readiness Guide. Smart Cities Council</dct:source>
        <rdfs:comment xml:lang="en">Connectivity is how the smart city’s devices communicate with each other. Connectivity ensures that data gets from where it is collected to where it is
analyzed and used. Examples include citywide WiFi networks, RF mesh networks and cellular networks. </rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontopolis.org/osmosys#Crowdsourcing -->

    <owl:Class rdf:about="http://www.ontopolis.org/osmosys#Crowdsourcing">
        <rdfs:subClassOf rdf:resource="http://www.ontopolis.org/osmosys#UrbanSensing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#isAbout"/>
                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#UrbanSystems"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#derivedFrom"/>
                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#SocialMediaPlatform"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ontopolis.org/osmosys#CulturalGroups -->

    <owl:Class rdf:about="http://www.ontopolis.org/osmosys#CulturalGroups">
        <rdfs:subClassOf rdf:resource="&foaf;/Organization"/>
