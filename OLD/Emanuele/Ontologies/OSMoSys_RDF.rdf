                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#Item"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontopolis.org/osmosys#makesDecision"/>
                <owl:someValuesFrom rdf:resource="http://www.ontopolis.org/osmosys#Decision"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
